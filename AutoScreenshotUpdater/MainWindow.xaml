<Window x:Class="AutoScreenshotUpdater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoScreenshotUpdater"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="400">
    <DockPanel LastChildFill="True">
        <TextBlock Text="Auto Screenshot Updater" FontSize="30" HorizontalAlignment="Center" FontWeight="Black" Padding="10" DockPanel.Dock="Top"/>
        <TextBlock Text="version 1.0.2" FontSize="20" HorizontalAlignment="Center" FontWeight="Black" Padding="10" DockPanel.Dock="Top"/>
        <Button x:Name="Update_Btn" Content="Update" Click="Update_Btn_Click"/>



    </DockPanel>
</Window>
